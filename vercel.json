{
    "version": 2,
    "builds": [
        {
            "src": "./index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": [
                "/(.*)",
                "/teste"
            ],
            "dest": "src/index.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        },
        {
            "src": [
                "/login"
            ],
            "dest": "src/index.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}